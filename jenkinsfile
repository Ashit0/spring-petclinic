pipeline{
    agent{
        label 'JDK_17'
    }
    triggers{ pollSCM( '* * * * *') }
    stages{
        stage('vcs'){
            steps{
            git url: 'https://github.com/Ashit0/spring-petclinic.git',
                branch: 'develop'
            } 
        } 
        stage('BUILD'){
            steps{
                sh './mvnw package'
            }
        }
        
    }

}